<article>
  <aside>
    <span class="article-number"></span>
  </aside>
  <section>
    <header>
      <span>{{ name }}</span>
      <br>
      <a href="{{ url }}" class="entry-title">{{ title }}</a>
      <span class="external-link-arrow"></span>
      <br>
      <span>{{ date }}</span>
    </header>
    <br><p>{{ description }} <a href="{{ url }}">READ MORE</a></p>
  </section>
</article>
